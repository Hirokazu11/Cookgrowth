<% provide(:title, "通知") %>
<div id="fh5co-portfolio">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h3>　通知一覧</h3>
      </div>
      <% if @notifications.present? %>
        <ol class="notifications">
          <%= render @notifications %>
        </ol>
        <%= will_paginate %>
      <% else %>
        <p>通知はありません</p>
      <% end %>
    </div>
  </div>
</div>